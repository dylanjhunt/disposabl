<div class="container">
  <div class="login-container  col-xs-12 col-sm-8 col-md-6 col-md-offset-3">
    <div id="output"></div>
    <div class="form-box">
      <%= image_tag "disposabl-logo.png" %>
      <h2>Log In</h2>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="signupform">
          <div class="field">
            <%= f.label :email %><br />
            <%= f.email_field :email, autofocus: true %>
          </div>
          <div class="field">
            <%= f.label :password %><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>
          <div class="field signup">
            <%= f.submit "Log in" %>
          </div>
        </div>
      <% end %>
      <div class="field">
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>
